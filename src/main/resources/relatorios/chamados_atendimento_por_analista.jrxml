	<queryString>
		<![CDATA[SELECT
	u.id,
	u.nome,
	u.email,
	COUNT(c.*)
FROM
	usuario u
	LEFT JOIN chamado c
		ON c.id_usuario_atendimento = u.id
		AND c.id_tipo_situacao = 2
WHERE
	u.id_papel = 2
GROUP BY
	u.id,
	u.nome,
	u.email
HAVING
	COUNT(c.*) > 0
ORDER BY
	u.nome]]>
	</queryString>
